    'os_requirements': '',
    'script_contents': '#!/bin/bash\n\n# Define paths\ncompose_file_path="/tmp/docker-compose.yml"\n\n# Get the hostname\nhost_username=$3\n\n# Write docker-compose.yml\ncompose_content=$(cat <<EOF\nservices:\n  api:\n    image: danielstairwell/ec2_api_test_data_arm_jammy:latest\n    network_mode: host\n    restart: always\n    volumes:\n      - /Users:/host_users\n      - /Volumes:/host_volumes\n      - /private:/host_private\n      - /tmp:/host_tmp\n      - /var/folders:/host_var_folders\n    environment:\n      - HOST_HOSTNAME=$host_hostname\nEOF\n)\n\n# Try to write the file\nif echo "$compose_content" > "$compose_file_path"; then\n    echo "Wrote Docker Compose configuration to $compose_file_path."\n    chmod 666 "$compose_file_path"\n    echo "Set permissions to read and write for everyone on $compose_file_path."\nelse\n    echo "An error occurred while writing the Docker Compose file."\n    exit 1\nfi\n\nsudo -u "$username" zsh -c "cd /tmp && docker compose up --detach"\n',
    'script_contents_encoded': ''
  }
